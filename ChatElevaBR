// Header

jQuery(window).scroll(function () {
    if (jQuery(this).scrollTop() > 0) {
        jQuery('#main-header').addClass('header-mod');
    } else {
        jQuery('#main-header').removeClass('header-mod');
    }
});

// Menu

jQuery(function () {
    jQuery(".menu-mobile").click(function () {
        jQuery("body").addClass("open-nav");
    });

    jQuery('.close-menu-mobile').click(function () {
        jQuery("body").removeClass('open-nav');
    });
});
</script>

